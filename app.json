{
  "expo": {
    "name": "FanceStyle",
    "slug": "FanceStyle",
    "scheme": "FanceStyle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoFancy.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoFancy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoFancy.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.matheusfer200.FanceStyle"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "4a236cb4-3e66-4df4-87f9-1855f00912c8"
      }
    }
  }
}
